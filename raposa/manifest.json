{
    "manifest_version": 2,
    "name": "raposa",
    "description": "macros",
    "browser_action":{
        "default_popup": "menu.html",
        "default_icon": "icon.png"
    },
    "version": "1.2",
    "permissions": [
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }

}